@model IEnumerable<WebUI.Model.NavModel>

<div id="nav" class="ui visible left vertical inverted labeled icon sidebar menu">
  <div class="ui black big launch right attached fixed button">
    <i class="content icon"></i>
    <span class="text">Menu</span>
  </div>
  @foreach (var menu in Model)
  {
     <a class="item" href="@menu.valuepath">
      <i class="@menu.MenuIcon"></i>
      @menu.value
     </a>
  }

